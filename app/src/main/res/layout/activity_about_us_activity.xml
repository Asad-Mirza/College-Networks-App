<RelativeLayout android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    xmlns:android="http://schemas.android.com/apk/res/android" >
<include
    android:id="@+id/app_bar"
    layout="@layout/app_bar_layout"
    android:layout_height="wrap_content"
    android:layout_width="match_parent"/>

    <ImageView
        android:layout_marginTop="4dp"
        android:layout_below="@id/app_bar"
        android:id="@+id/logo_image2"

        android:layout_centerHorizontal="true"


        android:layout_width="230dp"
        android:layout_height="70dp"
        android:src="@drawable/logo_with_text"
       />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/version"
        android:textSize="16sp"
        android:textColor="@color/materialTextColorPrimary"
        android:layout_centerHorizontal="true"
        android:layout_below="@id/logo_image2"
        android:text="Version 1.0 Beta"/>

    <TextView
        android:layout_marginTop="20dp"
        android:id="@+id/devby"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/version"


        android:layout_marginLeft="12dp"
        android:text="Developed by"
        android:textColor="@color/materialTextColorPrimary"
        android:textSize="16sp" />
    <android.support.v7.widget.CardView
        android:elevation="2dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/cardview1"

        android:layout_marginTop="8dp"
        android:layout_below="@id/devby">
        <RelativeLayout
            android:padding="8dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <TextView


                android:layout_marginTop="12dp"
                android:textColor="@color/materialTextColorPrimary"


                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Asad Mirza"
                android:id="@+id/name1"
                android:layout_marginLeft="8dp"
                android:textSize="18sp"/>
        <ImageView
            android:layout_marginTop="20dp"
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:id="@+id/fb"
            android:layout_marginLeft="8dp"
            android:layout_below="@id/name1"
            android:layout_marginBottom="12dp"
            android:src="@drawable/fb_icon"/>
            <ImageView
                android:layout_marginTop="20dp"
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:id="@+id/insta_icon"
                android:layout_below="@id/name1"
                android:layout_toRightOf="@id/fb"
                android:layout_marginBottom="12dp"
                android:layout_marginStart="20dp"
                android:src="@drawable/insta_icon"/>
            <ImageView
                android:layout_marginTop="20dp"
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:id="@+id/yt_icon"
                android:layout_below="@id/name1"
                android:layout_toRightOf="@id/insta_icon"
                android:layout_marginBottom="12dp"
                android:layout_marginStart="20dp"
                android:src="@drawable/if_youtube_317714"/>


        </RelativeLayout>
    </android.support.v7.widget.CardView>
    <android.support.v7.widget.CardView
        android:elevation="2dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/cardview2"

        android:layout_marginTop="10dp"
        android:layout_below="@id/cardview1">
    <RelativeLayout
        android:padding="8dp"

        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <TextView


            android:layout_marginTop="12dp"
            android:textColor="@color/materialTextColorPrimary"


            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Tosif Khan"
            android:id="@+id/name2"
            android:layout_marginLeft="8dp"
            android:textSize="18sp"/>
        <ImageView
            android:layout_marginTop="20dp"
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:id="@+id/fb2"
            android:layout_marginLeft="8dp"
            android:layout_marginBottom="12dp"
            android:layout_below="@id/name2"
            android:src="@drawable/fb_icon"/>
        <ImageView
            android:layout_marginTop="20dp"
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:id="@+id/insta_icon2"
            android:layout_below="@id/name2"
            android:layout_toRightOf="@id/fb2"
            android:layout_marginStart="20dp"
            android:layout_marginBottom="12dp"
            android:src="@drawable/insta_icon"/>


    </RelativeLayout>
</android.support.v7.widget.CardView>


</RelativeLayout>
